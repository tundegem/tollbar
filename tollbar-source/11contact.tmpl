LOCATION=contact.html
PARENT=ROOT
PAGENAME="Contact"
xTEMP=`mktemp`
cat >$xTEMP <<EOF
	<p style="text-align:center;">Askern Road<br />Toll Bar<br />Doncaster<br />South Yorkshire<br />DN5 0QR</p>
	<p style="text-align:center;"><strong><em>Telephone</em></strong>: (01302) 874324</p>
