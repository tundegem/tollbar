xTEMP=`mktemp`
cat >$xTEMP <<EOF
	<script src="http://maps.google.com/maps?file=api&amp;v=2&amp;key=ABQIAAAAlUxesxSydOb_SQF6o594BBQbOXjNkNblU885Zm4RaXqYVKsCiBSoai5cY1RHjsHxsYIk3lEReOYTYQ" type="text/javascript"></script>
	<script type="text/javascript">
		//<![CDATA[
		if (GBrowserIsCompatible()) {
			var map = new GMap2(document.getElementById("map"));
			map.setCenter(new GLatLng(53.565739, -1.154487), 13);
			var point = new GLatLng(53.565739, -1.154487);
			var school = new GMarker(point);
			map.addOverlay(school);
			school.bindInfoWindowHtml("Toll Bar School");
			map.setMapType(G_HYBRID_MAP);
			map.addControl(new GLargeMapControl());
		}
		//]]>
	</script>
EOF
EXTRAHEADER=`cat $xTEMP`
rm $xTEMP